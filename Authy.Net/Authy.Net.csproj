<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Title>Authy.Net</Title>
    <Product>Authy.Net</Product>
    <Company>Authy</Company>
    <Authors>Devin Martin</Authors>
    <VersionSuffix>$(VersionSuffix)</VersionSuffix>
    <Version>1.1.0.3</Version>
    <Version Condition=" '$(VersionSuffix)' != '' ">$(Version)-$(VersionSuffix)</Version>

    <PackageId>Authy.Net.NetStandard</PackageId>
    <PackageProjectUrl>https://github.com/mrtristan/Authy.Net.NetStandard</PackageProjectUrl>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="13.0.1" />
  </ItemGroup>
</Project>